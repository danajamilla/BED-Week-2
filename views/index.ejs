<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Homepage</title>
    <link rel="stylesheet" href="css/style.css"/>
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link
      href="https://fonts.googleapis.com/css2?family=Montserrat:wght@100;400;700&display=swap"
      rel="stylesheet"
    />
  </head>

  <body class="home">
    <%- include('partials/header.ejs') %>

    <img draggable="true" src="images/Labradoodle.jpeg"/>
    <!--- https://www.nieuwehond.nl/labradoodle/ -->

    <div class="naamblok">
      <% honden.forEach(function(hondenProfiel){ %>
        <p><%= hondenProfiel.naam %></p>
        <p><%= hondenProfiel.beschrijving %></p>
        <% }) %>
    </div>
 
    <div class="like">
      <button type="button">✖️</button>

      <a href="matchpagina">
        <button type="button">🐾</button>
      </a>
    </div>

    <%- include('partials/footer.ejs') %>

    <script src="script/script.js"></script>
  </body>
</html>
